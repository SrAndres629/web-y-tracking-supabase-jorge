{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "@vercel/python",
            "memory": 512,
            "maxDuration": 15
        }
    },
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ]
}
{
    "version": 2,
    "functions": {
        "api/index.py": {
            "includeFiles": [
                "api/templates/**",
                "static/dist/**",
                "static/assets/**",
                "static/engines/**",
                "static/css/**",
                "static/js/**"
            ]
        }
    },
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "redirects": [
        {
            "source": "/favicon.ico",
            "destination": "/static/images/luxury_logo.svg",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable, stale-while-revalidate=604800"
                }
            ]
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
}
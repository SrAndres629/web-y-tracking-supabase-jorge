{
    "version": 2,
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "functions": {
        "api/*.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "redirects": [
        {
            "source": "/favicon.ico",
            "destination": "/static/images/luxury_logo.svg",
            "permanent": true
        },
        {
            "source": "/favicon.png",
            "destination": "/static/images/luxury_logo.svg",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/_vercel/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                }
            ]
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
}
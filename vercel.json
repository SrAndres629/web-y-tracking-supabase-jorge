{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "images": {
        "sizes": [
            640,
            750,
            828,
            1080,
            1200,
            1920,
            2048
        ],
        "domains": [
            "jorgeaguirreflores.com"
        ],
        "formats": [
            "image/avif",
            "image/webp"
        ],
        "minimumCacheTTL": 60
    },
    "functions": {
        "api/index.py": {
            "includeFiles": "{app,api/templates,static}/**,main.py,.env*"
        }
    },
    "rewrites": [
        {
            "source": "/favicon.ico",
            "destination": "/static/assets/images/branding/luxury_logo.svg"
        },
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable, stale-while-revalidate=604800"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
}